<body class="sb-nav-fixed">
    
  <%- include('../partials/admin-header')%>
    <div id="h">
      <div class="container-fluid px-4">
        <h1 class="mt-4">User data</h1> 
        
  </div>
     <div class="container-fluid">
      <% if(userdatashow!=null) { %>
      <table id="example" class="table table-bordered table-striped ">
      
        <thead>
          <tr>
             
            <th>Name</th>
            <th>Email</th>
            
            <th>phone</th>
            <th>Status</th>
            
            </th>

        </tr>
        </thead>
        <tbody>
          <% userdatashow.forEach((productData)=>{ %>
            <tr>
              <td><%=productData.name%></td>
              <td><%=productData.email%></td>
              
              <td><%=productData.phone%></td>
             
              
            
    
              <td>    
                <% if(productData.status==true) {%>
                    <a class="btn btn-warning btn-sm" href="/userdatablock?id=<%= productData._id%>" role="button">Block</a>
                <%}else{%>
                    <a class="btn btn-success btn-sm" href="/userdatablock?id=<%= productData._id%>" role="button">Unblock</a>
                <%}%>
                </td>
    
              
            </tr>
            <%})%>
           
              <tbody>
                <%}%>
                 
     </div>
      
    </div>
    <!-- </div> -->








    
    <script>
      $(document).ready(function () {
        var table = $('#example').DataTable({
          rowReorder: {
            selector: 'td:nth-child(2)'
          },
          responsive: true
        });
      });
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
